<ul>
<% Attraction.all.each do |attraction| %>
  <%= link_to "Go on #{attraction.name}", attraction_path(attraction) unless @user.admin == true %>
  <%= link_to "Show #{attraction.name}", attraction_path(attraction) unless @user.admin == false %>
<% end %>
</ul>


<% if @user.admin %>
  <%= link_to 'New Attraction', new_attraction_path %>
<% end %>
